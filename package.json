{
    "name": "kodus-web",
    "version": "1.0.0",
    "description": "Kodus Web Application - Open Source Development Platform",
    "keywords": [
        "kodus",
        "web",
        "development",
        "platform",
        "kody"
    ],
    "author": "Kodus Tech",
    "scripts": {
        "start:dev": "next dev --turbopack --port 3000",
        "dev:first-run": "bash scripts/setup-dev-env.sh",
        "build": "next build",
        "start": "next start",
        "start:qa": "NODE_ENV=production next start",
        "lint": "eslint . --config eslint.config.js",
        "check-types": "npx tsc --noEmit",
        "build-analyze": "cross-env ANALYZE=true yarn build",
        "prettier": "prettier --write **/*.{js,jsx,ts,tsx}",
        "format": "prettier --write .",
        "docker:build": "docker compose -f docker-compose.dev.yml down -v && docker volume rm kodus-web_node_modules_cache 2>/dev/null || true && docker compose -f docker-compose.dev.yml build",
        "docker:start": "docker compose -f docker-compose.dev.yml up",
        "docker:clean": "docker compose -f docker-compose.dev.yml down -v",
        "docker:clean-cache": "docker volume rm kodus-web_node_modules_cache",
        "docker:reset": "yarn docker:clean && yarn docker:start"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.2",
